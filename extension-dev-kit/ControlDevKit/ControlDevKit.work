<?xml version="1.0" encoding="UTF-8"?>

<application>
  <option name="excludedirs" value="cvs;sccs;rcs;" />
  <option name="excludefiles" value="\..*|.*\.[0-9]+|wlw_[0-9]+\..*" />
  <option name="name" value="ControlDevKit" />
  <option name="prerelease-version" value="2" />
  <option name="server.path" value="${weblogic.home}/samples/domains/workshop" />
  <option name="version" value="1" />
  <components>
    <component name="com.bea.ide.debug.IDebugClient">
      <option name="SrcPath0_currproject_" value="Current Project" />
      <option name="SrcPath1_project_" value="ControlTest" />
      <option name="SrcPath2_project_" value="ControlFeatures" />
      <option name="SrcPath3_project_" value="DBScripter" />
    </component>
    <component name="com.bea.ide.workspace.IWorkspace">
      <node name="ApplicationBuild">
        <node name="1">
          <option name="includeProject" value="true" />
          <option name="project" value="ControlFeatures" />
        </node>
        <node name="2">
          <option name="includeProject" value="true" />
          <option name="project" value="DBScripter" />
        </node>
        <node name="3">
          <option name="includeProject" value="true" />
          <option name="project" value="ControlTest" />
        </node>
        <node name="4">
          <option name="includeProject" value="true" />
          <option name="project" value="ControlAppInstall" />
        </node>
      </node>
    </component>
    <component name="com.bea.portal.ide.actions.OpenAdministrationPageAction" />
    <component name="com.bea.portal.ide.actions.OpenAsXMLAction" />
  </components>
  <projects>
    <project name="ControlFeatures">
      <option name="project.path" value="./ControlFeatures" />
      <option name="type" value="urn:com-bea-ide:project.type:Control" />
      <components>
        <component name="com.bea.ide.JavaCompiler" />
        <component name="com.bea.ide.debug.IDebugData">
          <option name="debug.appendLibrariesClassPath" value="true" />
          <option name="debug.appendServerClassPath" value="true" />
          <option name="debug.attachProc.server" value="" />
          <option name="debug.attachProc.shmemAddress" value="" />
          <option name="debug.attachProc.type" value="0" />
          <option name="debug.buildBeforeDebugging" value="false" />
          <option name="debug.class.path" value="${workshop.home}/wlw-ide.jar;${workshop.home}/lib/jdom.jar;${workshop.home}/lib/wlw-ide-schemas.jar" />
          <option name="debug.debuggingType" value="NewProcess" />
          <option name="debug.home.path" value="${workshop.home}" />
          <option name="debug.main" value="workshop.core.Workshop" />
          <option name="debug.params" value="" />
          <option name="debug.port" value="" />
          <option name="debug.smartDebugging" value="true" />
          <option name="debug.vmParams" value="-ea -Xmx512m -Xms128m -Dsun.java2d.d3d=false -Djava.system.class.loader=workshop.core.AppClassLoader" />
          <node name="com.bea.ide.debug.IDebugData.classList">
            <option name="Generated classes" value="$Proxy*, *_wf$*, *_wf" />
            <option name="JDK classes" value="javax.*, java.*, sun.*, com.sun.*" />
            <option name="WebLogic Server classes" value="weblogic.*, weblogicx.*, com.pointbase.*, com.bea.*, COM.jrockit, com.octetstring.*, com.bluecast.*, netscape.ldap.*" />
            <option name="WebLogic Workshop classes" value="knex.*, workshop.*, javelin.*" />
            <option name="XML classes" value="org.apache.*, XMLRuntime.*, org.xml.*, org.xmlsoap.schemas.*" />
          </node>
        </component>
        <component name="workshop.workspace.project.build.ProjectBuilderPrefs">
          <option name="ant.classpath" value="" />
          <option name="ant.path" value="${ControlFeatures.root}/build.xml" />
          <option name="buildTarget" value="build" />
          <option name="useAnt" value="false" />
        </component>
      </components>
    </project>
    <project name="ControlTest">
      <option name="project.path" value="./ControlTest" />
      <option name="type" value="urn:com-bea-ide:project.type:WebApp" />
      <components>
        <component name="com.bea.ide.JavaCompiler" />
        <component name="com.bea.ide.debug.IDebugData" />
        <component name="workshop.workspace.project.build.ProjectBuilderPrefs" />
      </components>
    </project>
    <project name="DBScripter">
      <option name="project.path" value="./DBScripter" />
      <option name="type" value="urn:com-bea-ide:project.type:Control" />
      <components>
        <component name="com.bea.ide.JavaCompiler" />
        <component name="com.bea.ide.debug.IDebugData">
          <option name="debug.appendLibrariesClassPath" value="true" />
          <option name="debug.appendServerClassPath" value="true" />
          <option name="debug.attachProc.server" value="" />
          <option name="debug.attachProc.shmemAddress" value="" />
          <option name="debug.attachProc.type" value="0" />
          <option name="debug.buildBeforeDebugging" value="false" />
          <option name="debug.class.path" value="${workshop.home}/wlw-ide.jar;${workshop.home}/lib/jdom.jar;${workshop.home}/lib/wlw-ide-schemas.jar" />
          <option name="debug.debuggingType" value="NewProcess" />
          <option name="debug.home.path" value="${workshop.home}" />
          <option name="debug.main" value="workshop.core.Workshop" />
          <option name="debug.params" value="D:\P4dev2\samples\workshop\ExtensionDevKit" />
          <option name="debug.port" value="" />
          <option name="debug.smartDebugging" value="false" />
          <option name="debug.vmParams" value="-ea -Xmx512m -Xms128m -Dsun.java2d.d3d=false -Djava.system.class.loader=workshop.core.AppClassLoader" />
          <node name="com.bea.ide.debug.IDebugData.classList">
            <option name="Generated classes" value="$Proxy*, *_wf$*, *_wf" />
            <option name="JDK classes" value="javax.*, java.*, sun.*, com.sun.*" />
            <option name="WebLogic Server classes" value="weblogic.*, weblogicx.*, com.pointbase.*, com.bea.*, COM.jrockit, com.octetstring.*, com.bluecast.*, netscape.ldap.*" />
            <option name="WebLogic Workshop classes" value="knex.*, workshop.*, javelin.*" />
            <option name="XML classes" value="org.apache.*, XMLRuntime.*, org.xml.*, org.xmlsoap.schemas.*" />
          </node>
        </component>
        <component name="workshop.workspace.project.build.ControlBuilderPrefs">
          <option name="deliverable.docs.directory" value="docs" />
        </component>
        <component name="workshop.workspace.project.build.ProjectBuilderPrefs">
          <option name="ant.classpath" value="" />
          <option name="ant.path" value="" />
          <option name="useAnt" value="false" />
        </component>
      </components>
    </project>
  </projects>
  <node name="roles" />
</application>

